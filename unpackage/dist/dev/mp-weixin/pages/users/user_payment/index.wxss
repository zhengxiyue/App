.user_payment {
  height: 100vh;
  background-color: #fff;
}
.payment {
  position: relative;
  top: -60rpx;
  width: 100%;
  background-color: #fff;
  border-radius: 10rpx;
  padding-top: 25rpx;
  border-top-right-radius: 14rpx;
  border-top-left-radius: 14rpx;
}
.payment .nav {
  height: 75rpx;
  line-height: 75rpx;
  padding: 0 100rpx;
}
.payment .nav .item {
  font-size: 30rpx;
  color: #333;
}
.payment .nav .item.on {
  font-weight: bold;
  border-bottom: theme;
}
[data-theme="theme1"] .payment .nav .item.on {
  border-bottom: 4rpx solid #e93323 !important;
}
[data-theme="theme2"] .payment .nav .item.on {
  border-bottom: 4rpx solid #FE5C2D !important;
}
[data-theme="theme3"] .payment .nav .item.on {
  border-bottom: 4rpx solid #42CA4D !important;
}
[data-theme="theme4"] .payment .nav .item.on {
  border-bottom: 4rpx solid #1DB0FC !important;
}
[data-theme="theme5"] .payment .nav .item.on {
  border-bottom: 4rpx solid #FF448F !important;
}
.payment .input {
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 1px dashed #dddddd;
  margin: 60rpx auto 0 auto;
  padding-bottom: 20rpx;
  font-size: 56rpx;
  color: #333333;
  flex-wrap: nowrap;
}
.payment .input text {
  padding-left: 106rpx;
}
.payment .input input {
  padding-right: 106rpx;
  width: 300rpx;
  height: 94rpx;
  text-align: center;
  font-size: 70rpx;
}
.payment .placeholder {
  color: #d0d0d0;
  height: 100%;
  line-height: 94rpx;
}
.payment .tip {
  font-size: 26rpx;
  color: #888888;
  padding: 0 30rpx;
}
.payment .but {
  color: #fff;
  font-size: 30rpx;
  width: 700rpx;
  height: 86rpx;
  border-radius: 43rpx;
  margin: 50rpx auto 0 auto;
  background: theme;
  line-height: 86rpx;
}
[data-theme="theme1"] .payment .but {
  background: linear-gradient(90deg, #FF7931 0%, #E93323 100%);
}
[data-theme="theme2"] .payment .but {
  background: linear-gradient(90deg, #FF9445 0%, #FE5C2D 100%);
}
[data-theme="theme3"] .payment .but {
  background: linear-gradient(55deg, #70E038 0%, #42Ca4D 100%);
}
[data-theme="theme4"] .payment .but {
  background: linear-gradient(90deg, #40D1F4 0%, #1DB0FC 100%);
}
[data-theme="theme5"] .payment .but {
  background: linear-gradient(90deg, #FF448F 0%, #FF67AD 100%);
}
.payment-top {
  width: 100%;
  height: 350rpx;
  background-color: theme;
}
[data-theme="theme1"] .payment-top {
  background-color: #e93323 !important;
}
[data-theme="theme2"] .payment-top {
  background-color: #FE5C2D !important;
}
[data-theme="theme3"] .payment-top {
  background-color: #42CA4D !important;
}
[data-theme="theme4"] .payment-top {
  background-color: #1DB0FC !important;
}
[data-theme="theme5"] .payment-top {
  background-color: #FF448F !important;
}
.payment-top .name1 {
  font-size: 26rpx;
  color: rgba(255, 255, 255, 0.8);
  margin-top: -38rpx;
  margin-bottom: 30rpx;
}
.payment-top .pic {
  font-size: 32rpx;
  color: #fff;
}
.payment-top .pic-font {
  font-size: 78rpx;
  color: #fff;
}
.picList {
  display: flex;
  flex-wrap: wrap;
  margin: 30rpx 0;
}
.picList .pic-box {
  width: 32%;
  height: auto;
  border-radius: 20rpx;
  margin-top: 21rpx;
  padding: 20rpx 0;
  margin-right: 12rpx;
}
.picList .pic-box:nth-child(3n) {
  margin-right: 0;
}
.picList .pic-box-color {
  background-color: #f4f4f4;
  color: #656565;
}
.picList .pic-number {
  font-size: 22rpx;
}
.picList .pic-number-pic {
  font-size: 38rpx;
  margin-right: 10rpx;
  text-align: center;
}
.pic-box-color-active {
  background: theme;
  color: #fff !important;
}
[data-theme="theme1"] .pic-box-color-active {
  background: linear-gradient(90deg, #FF7931 0%, #E93323 100%);
}
[data-theme="theme2"] .pic-box-color-active {
  background: linear-gradient(90deg, #FF9445 0%, #FE5C2D 100%);
}
[data-theme="theme3"] .pic-box-color-active {
  background: linear-gradient(55deg, #70E038 0%, #42Ca4D 100%);
}
[data-theme="theme4"] .pic-box-color-active {
  background: linear-gradient(90deg, #40D1F4 0%, #1DB0FC 100%);
}
[data-theme="theme5"] .pic-box-color-active {
  background: linear-gradient(90deg, #FF448F 0%, #FF67AD 100%);
}
.tips-box .tips {
  font-size: 28rpx;
  color: #333333;
  font-weight: 800;
  margin-bottom: 14rpx;
  margin-top: 20rpx;
}
.tips-box .tips-samll {
  font-size: 24rpx;
  color: #333333;
  margin-bottom: 14rpx;
}
.tips-box .tip-box {
  margin-top: 30rpx;
}
.tips-title {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #333;
}
.wrapper .item textarea {
  background-color: #f9f9f9;
  width: auto !important;
  height: 140rpx;
  border-radius: 14rpx;
  margin-top: 30rpx;
  padding: 15rpx;
  box-sizing: border-box;
  font-weight: 400;
}
.px-30 {
  padding-left: 30rpx;
  padding-rigt: 30rpx;
}
.wrapper .item .placeholder {
  color: #ccc;
}
.wrapper .item .list {
  margin-top: 35rpx;
}
.wrapper .item .list .payItem {
  border: 1px solid #eee;
  border-radius: 14rpx;
  height: 86rpx;
  width: 95%;
  box-sizing: border-box;
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #282828;
}
.wrapper .item .list .payItem.on {
  border: theme;
  color: #E93323;
}
[data-theme="theme1"] .wrapper .item .list .payItem.on {
  border: 1px solid #e93323;
}
[data-theme="theme2"] .wrapper .item .list .payItem.on {
  border: 1px solid #FE5C2D;
}
[data-theme="theme3"] .wrapper .item .list .payItem.on {
  border: 1px solid #42CA4D;
}
[data-theme="theme4"] .wrapper .item .list .payItem.on {
  border: 1px solid #1DB0FC;
}
[data-theme="theme5"] .wrapper .item .list .payItem.on {
  border: 1px solid #FF448F;
}
.name {
  width: 50%;
  text-align: center;
  border-right: 1px solid #eee;
}
.name .iconfont {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 44rpx;
  background-color: #fe960f;
  color: #fff;
  font-size: 30rpx;
  margin-right: 15rpx;
}
.name .iconfont.icon-weixin2 {
  background-color: #41b035;
}
.name .iconfont.icon-zhifubao {
  background-color: #00AAEA;
}
.payItem .tip {
  width: 49%;
  text-align: center;
  font-size: 26rpx;
  color: #aaa;
}

