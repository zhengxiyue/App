
.app_update{
	background-color: #fff;
	height: 100vh;
}
.logo_box{
	height: 500rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.logo_box image{
	display: block; 
	margin-top:80rpx; 
	width: 120rpx;
	height: 120rpx;
	border-radius: 50%;
}
.title{
	font-size: 34rpx;
	font-family: PingFang SC;
	font-weight: 600;
	color: #333333;
	margin: 20rpx auto 20rpx;
}
.version{
	font-size: 28rpx;
	font-family: PingFang SC;
	font-weight: 500;
	color: #333333;
}
.jiancha{
	width: 690rpx;
	margin: 20rpx auto 0;
	padding: 0 20rpx 0;
	height: 100rpx;
	line-height: 100rpx;
	color: #333333;
	font-size: 30rpx;
	border-top:1px solid #f5f5f5;
	border-bottom:1px solid #f5f5f5;
	display: flex;
	justify-content: space-between;
}

